struct A {
    a: i32,
    b: i32,
}

struct B {
    a: A,
}

fn main() {
    () = ();
    (_) = 1;
    let mut b: i32 = 0;
    A { a: _, b } = A { a: 1, b: 2 };
    printlnInt(b);
    B { a: A { a: _, b } } = B {
        a: A { a: 1, b: 3 },
    };
    printlnInt(b);
    [_, b] = [1, 4];
    printlnInt(b);

    exit(0);
}
