// trait 默认方法实现
struct Counter {
    value: i32,
}

trait Incrementable {
    fn get(&self) -> i32;
    
    fn double(&self) -> i32 {
        self.get() * 2
    }
    
    fn triple(&self) -> i32 {
        self.get() * 3
    }
}

impl Incrementable for Counter {
    fn get(&self) -> i32 {
        self.value
    }
}

fn main() {
    let c: Counter = Counter { value: 7 };
    printlnInt(c.get());
    printlnInt(c.double());
    printlnInt(c.triple());
    exit(0);
}
